@model Tutoronic.Response.GetAllAdminsResponse

@{
    ViewBag.Title = "alladmins";
}

<h2>alladmins</h2>

<p>
    @Html.ActionLink("Register","Register","Home")
</p>
<table class="table">
    <tr>
        <th>
            <b>Name</b>
        </th>
        <th>
            <b>Email</b>
        </th>
        <th>
            <b>Image</b>
        </th>
        <th>
            <b>Contact</b>
        </th>
        <th>
            <b>Address</b>
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model.AllAdmin)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AdminName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AdminEmail)
            </td>
            <td>
                <img src="@Url.Content(item.AdminPic)" width="120" height="120" />
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AdminAddress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AdminContact)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.AdminId }) |
                @Html.ActionLink("Details", "Details", new { id = item.AdminId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.AdminId })
            </td>
        </tr>
    }

</table>
